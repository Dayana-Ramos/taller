<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="proyecto.Opciones" table="Opciones" lazy="false">
		<id name="idopcion" column="Idopcion" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="opcion" column="Opcion" type="string" length="255" not-null="true" lazy="false"/>
		<property name="correcta" column="Correcta" type="boolean" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
